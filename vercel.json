{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/server/index.ts" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "functions": {
    "server/index.ts": {
      "runtime": "node:20.x"
    }
  }
}
